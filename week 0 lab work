#include <iostream>
using namespace std;

int main()
{
  int t;
  cin >> t;
  while(t--)
  {
    int n;
    cin >> n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
      cin>>arr[i];
    }
    int s; // element to search
    cin >> s;
    bool flag=false;
   
    int c=0;
    for(int i=0;i<n;i++)
    {
      c++;
      if(arr[i] == s)
      {
        flag=true;
        cout <<"element is present at "<< i << " position counter is  " << counter << "\n";
        break;
      }
    }
    if(!flag){
      cout << "element is not present " << "\n";
    }
  }
  return 0;
}
